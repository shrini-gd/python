def hey(phrase):
	phrase = phrase.strip()
	if phrase.isupper() == True:
		if phrase.endswith('?') == True:
			return "Calm down, I know what I'm doing!"
		else:
			return 'Whoa, chill out!'
	elif phrase.endswith('?') == True:
		return 'Sure.'	
	elif len(phrase) == 0:
		return  'Fine. Be that way!' 
	else:
		return 'Whatever.'
